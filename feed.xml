---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Mediatec Insights</title>
    <description>Strategic research and data intelligence.</description>
    <link>https://insights.mediatec.africa/</link>
    <atom:link href="https://insights.mediatec.africa/feed.xml" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    {% for post in site.posts limit:10 %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <description>{{ post.excerpt | xml_escape }}</description>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
      <link>https://insights.mediatec.africa{{ post.url }}</link>
      <guid isPermaLink="true">https://insights.mediatec.africa{{ post.url }}</guid>
      {% if post.image %}
      <enclosure url="https://insights.mediatec.africa{{ post.image }}" length="0" type="image/jpeg" />
      {% endif %}
    </item>
    {% endfor %}
  </channel>
</rss>
